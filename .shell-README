# Append to platform specific Bash files
#  - Linux: ~/.bashrc
#  - Mac: ~/.bash_profile, ~/.zprofile
#  - zsh: ~/.zshrc

if [ -f ~/.shell-custom ]; then
    . ~/.shell-custom
fi

if [ -f ~/.shell-custom-linux ]; then
    . ~/.shell-custom-linux
fi

if [ -f ~/.shell-custom-macos ]; then
    . ~/.shell-custom-macos
fi
